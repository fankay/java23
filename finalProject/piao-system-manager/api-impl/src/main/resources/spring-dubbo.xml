<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:bean="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="Piao-System-Manager"/>

    <dubbo:registry protocol="zookeeper" address="192.168.135.136:2181,192.168.135.136:2182,192.168.135.136:2183"/>

    <!--固定端口和ip-->
    <dubbo:protocol name="dubbo" port="20880" host="192.168.1.112"/>

    <!--用户服务-->
    <dubbo:service interface="com.kaishengit.piao.systemmanager.api.UserService" ref="userServiceImpl"/>

</beans>